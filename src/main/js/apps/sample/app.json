        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "allowedBundles": [
            "console",
            "system@^4.6.0",
            "splashscreen@^4.6.0",
            "templatelayout@^4.6.0",
            "template-seasons@^4.6.0",
            "theme-everlasting@^4.6.0",
            "toolset@^4.6.0",
            "map-init@^4.6.0",
            "toc",
            "agssearch@^4.6.0",
            "dn_conditionalrenderer"
        ]
    },
    "bundles": {
        "dn_conditionalrenderer": {
            "Config": {
                "layerRendererScalesMapping": [
                    {
                        "layerId": "unternehmen",
                        "fallbackRenderer": {
                            "type": "simple",
                            "visualVariables": [
                                {
                                    "type": "size",
                                    "field": "usto",
                                    "minDataValue": 1,
                                    "maxDataValue": 85,
                                    "minSize": 2,
                                    "maxSize": 30
                                }
                            ],
                            "symbol": {
                                "type": "simple-marker",
                                "color": [
