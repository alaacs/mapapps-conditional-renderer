                                    61,
                                    255
                                ],
                                "angle": 0,
                                "xoffset": 0,
                                "yoffset": 0,
                                "size": 4,
                                "style": "circle",
                                "outline": {
                                    "type": "simple-line",
                                    "color": [
                                        0,
                                        0,
                                        0,
                                        255
                                    ],
                                    "width": 0.7,
                                    "style": "solid"
                                }
                            }
                        },
                        "scaleRenderers": [
                            {
                                "scaleFrom": 3000,
                                "scaleTo": 50000,
                                "renderer": {
                                    "type": "heatmap",
                                    "field": "usto",
                                    "colorStops": [
                                        {
                                            "color": "rgba(115, 0, 115, 0)",
                                            "ratio": 0
